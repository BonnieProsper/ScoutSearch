{
  "dataset": "datasets/abc_news/sample.jsonl",
  "ranking": {
    "type": "robust",
    "params": {
      "tf_weight": 0.4,
      "tfidf_weight": 0.6
    }
  },
  "benchmark": {
    "k": 10,
    "warmup": 2,
    "repeats": 5
  },
  "queries": [
    {
      "query": "machine learning",
      "relevant_doc_ids": ["doc1", "doc23"]
    },
    {
      "query": "data science",
      "relevant_doc_ids": ["doc5"]
    }
  ],
  "output": "data/benchmarks/abc_news_robust.json"
}
